			<script>
				var intercept = function(data, length, isFill) {
					if (data == null || data == '') {
						return '_';
					}
				
					if (length > data.length) {
						return data;
					} else {
						var s = data.substr(0, length);
						if (isFill) return s + "...";
						return s;
					}
				};
				juicer.register('intercept', intercept);
				var toBoss = function(data, length, isFill) {
					if (length > data.length) {
						return data;
					} else {
						var s = data.substr(0, length);
						if (isFill) return s + "**";
						return s;
					}
				};
				juicer.register('toBoss', toBoss);
			
			</script>
